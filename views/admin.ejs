    <script src="/js/dependencies/angular-ui-notification.js"></script>
    <script src="/js/dependencies/angular-ui-notification.min.js"></script>
    <script src="/js/dependencies/bootstrap.js"></script>
    <script src="/js/dependencies/bootstrap.min.js"></script>
    <script src="/js/dependencies/d17ba647fb.js"></script>
    <script src="/js/dependencies/loading-bar.js"></script>
    <script src="/js/dependencies/underscore.js"></script>
    <script src="/js/dependencies/z-admin.js"></script>
    <script src="/js/dependencies/z-app.js"></script>
