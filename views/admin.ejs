        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav navbar-nav">
            <li>
              <a href="/admin/"> Productos</a>
            </li>
            <li>
              <a href="/admin/users"> </i>Usuarios</a>
            </li>
            <li>
              <a href="/admin/sales"> </i>Ventas</a>
          </li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li>
              <a href="/logout"> <i class="fa fa-user" aria-hidden="true"></i>
                <%= req.user.username %> | logout</a>
            </li>
          </ul>
        </div>
        <!-- /.navbar-collapse -->
      </div>
      <!-- /.container -->
    </nav>




    <%- body %>

    <footer style="margin:0px; text-align:center;">
      <div class="row">
        <div class="col-lg-12">
          <p>Copyright &copy; Test Grimorum 2016</p>
        </div>
